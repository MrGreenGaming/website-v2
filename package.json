{
  "name": "mrgreengaming-website",
  "version": "1.0.0",
  "description": "Website for MrGreenGaming",
  "author": "AfuSensi",
  "private": true,
  "scripts": {
    "prod": "pm2 start ecosystem.config.js",
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node ./server/index.js",
    "startnew": "nuxt-start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "@nuxtjs/auth": "^4.8.5",
    "@nuxtjs/axios": "^5.9.0",
    "@nuxtjs/proxy": "^1.3.3",
    "adm-zip": "^0.4.13",
    "basic-ftp": "^3.8.7",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cross-env": "^5.2.1",
    "delay": "^4.3.0",
    "env-cmd": "^8.0.2",
    "express": "^4.17.1",
    "fs-extra": "^7.0.1",
    "gamedig": "^3.0.5",
    "get-mp3-duration": "^1.0.0",
    "lodash": "^4.17.19",
    "md5-file": "^4.0.0",
    "moment": "^2.24.0",
    "mtasa": "^0.1.3",
    "multer": "^1.4.2",
    "mysql": "^2.17.1",
    "node-cache": "^4.2.1",
    "nuxt": "^2.9.2",
    "nuxt-clipboard2": "^0.2.1",
    "nuxt-start": "^2.11.0",
    "qs": "^6.9.1",
    "ssh2-sftp-client": "^5.1.2",
    "vue": "^2.6.11",
    "vue-paypal-checkout": "https://github.com/AfuSensi/vue-paypal-checkout/tarball/master",
    "vue-timeago": "^5.1.2",
    "vuetify": "^1.5.21",
    "vuetify-loader": "^1.4.3",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^0.0.1",
    "babel-eslint": "^8.2.6",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^3.6.0",
    "eslint-config-standard": "^13.0.1",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jest": "^22.21.0",
    "eslint-plugin-node": "^9.2.0",
    "eslint-plugin-prettier": "^2.7.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^5.2.3",
    "nodemon": "^1.19.4",
    "prettier": "^1.19.1",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/recommended",
      "eslint:recommended",
      "plugin:prettier/recommended",
      "prettier/vue"
    ],
    "rules": {
      "vue/component-name-in-template-casing": [
        "error",
        "PascalCase",
        {
          "ignores": [
            "nuxt",
            "nuxt-link"
          ]
        }
      ],
      "no-console": [
        "error",
        {
          "allow": [
            "log",
            "error",
            "warn"
          ]
        }
      ]
    },
    "globals": {
      "$nuxt": true
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  }
}
